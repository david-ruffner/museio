{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [        
        {
            "name": "Launch Books Microservice",
            "program": "${workspaceFolder}/museio_backend/books.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "pwa-chrome",
            "url": "https://museio.davidr.pro/*",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Launch Users Microservice",
            "program": "${workspaceFolder}/museio_backend/users.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "name": "Launch Tokenauthor API",
            "program": "${workspaceFolder}/museio_backend/tokenauthor.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },

        {
            "name": "Launch Dashboard API",
            "program": "${workspaceFolder}/museio_backend/dashboard.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },

        {
            "name": "Launch Song Bank API",
            "program": "${workspaceFolder}/museio_backend/songbank.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        }
    ]
}